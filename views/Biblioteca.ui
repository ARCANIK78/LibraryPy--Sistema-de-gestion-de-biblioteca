           <property name="styleSheet">
            <string notr="true">
QWidget {
    background-color: rgb(255, 255, 255); /* Fondo blanco */
}

QLabel {
    font: 80 12pt &quot;Arial Black&quot;;
    color: rgb(50, 50, 50); /* Texto en gris oscuro */
    border: 0px;
}

QLineEdit {
    border: 1px solid rgb(0, 0, 0); /* Borde negro */
    background-color: rgb(245, 245, 245); /* Fondo ligeramente gris */
    color: rgb(50, 50, 50); /* Texto gris oscuro */
    border-radius: 5px; /* Bordes redondeados */
    font: 75 12pt &quot;Times New Roman&quot;;
    padding: 5px; /* Espaciado interno */
}

QLineEdit:focus {
    border: 2px solid rgb(0, 0, 0); /* Borde negro más grueso al enfocar */
    background-color: rgb(255, 255, 255); /* Fondo blanco al enfocar */
}

QPushButton {
    background-color: transparent; /* Sin fondo */
    border-radius: 10px; /* Bordes redondeados */
    color: rgb(0, 0, 0); /* Texto negro */
    font: 60 9pt &quot;Arial Black&quot;; /* Fuente en negrita */
    padding: 5px 10px; /* Espaciado interno */
}


QPushButton:hover {
    background-color: transparent; /* Sin fondo al pasar el cursor */
    color: rgb(0, 0, 0); /* Texto negro */
}

QPushButton:pressed {
    background-color: transparent; /* Sin fondo al presionar */
    color: rgb(0, 0, 0); /* Texto negro */
}

QPushButton:disabled {
    background-color: transparent; /* Sin fondo */
    color: rgb(150, 150, 150); /* Texto gris claro */
}
/* spinxbox*/
QSpinBox {
    background-color: rgb(255, 255, 255); /* Fondo blanco */
    border: 1px solid rgb(200, 200, 200); /* Borde gris claro */
    border-radius: 5px; /* Bordes redondeados */
    color: rgb(50, 50, 50); /* Texto gris oscuro */
    font: 75 12pt &quot;Arial&quot;; /* Fuente simple */
    padding: 5px 10px; /* Espaciado interno */
}

QSpinBox:focus {
    border: 1px solid rgb(0, 123, 255); /* Borde azul al enfocar */
}


QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    color: rgb(0, 92, 204); /* Azul oscuro al pasar el cursor */
}
/*DateEdit*/
QDateEdit {
    background-color: rgb(255, 255, 255); /* Fondo blanco */
    border: 1px solid rgb(200, 200, 200); /* Borde gris claro */
    border-radius: 5px; /* Bordes redondeados */
    color: rgb(50, 50, 50); /* Texto gris oscuro */
    font: 75 12pt &quot;Arial&quot;; /* Fuente simple */
    padding: 5px 10px; /* Espaciado interno */
    width: 150px; /* Ancho fijo */
}
QDateEdit:focus {
    border: 1px solid rgb(0, 123, 255); /* Borde azul al enfocar */
	background-color: rgb(240, 240, 240); /* Fondo ligeramente gris al enfocar */
}
QDateEdit::down-arrow:hover {
    background-color: rgb(240, 240, 240); /* Fondo al pasar el cursor */
}


/* Combobox*/
QComboBox {
    
    border: 1px solid rgb(200, 200, 200); /* Borde gris claro */
    border-radius: 5px; /* Bordes redondeados */
    color: rgb(50, 50, 50); /* Texto gris oscuro */
    font: 75 12pt &quot;Arial&quot;; /* Fuente simple */
    padding: 5px 10px; /* Espaciado interno */
    min-width: 150px; /* Ancho mínimo */
    selection-background-color: rgb(0, 123, 255); /* Color de fondo de la selección */
}

QComboBox:focus {
    border: 1px solid rgb(0, 123, 255); /* Borde azul al enfocar */
    background-color: rgb(240, 240, 240); /* Fondo ligeramente gris al enfocar */
}
QComboBox::down-arrow:hover {
    background-color: rgb(0, 123, 255); /* Fondo azul al pasar el cursor */
    border-left: 4px solid rgb(255, 255, 255); /* Flecha blanca cuando el cursor pasa sobre ella */
}

</string>
